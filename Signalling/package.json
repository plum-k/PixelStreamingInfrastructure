    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "logform": "^2.7.0",
    "nodemon": "^3.1.9",
    "openapi-typescript": "^7.4.4",
    "prettier": "3.4.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.5",
